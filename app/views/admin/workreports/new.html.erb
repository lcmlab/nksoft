<% @page_title = "勤務報告情報の新規登録" %>
<h1><%= @page_title %></h1>

<h2><%= @employee.name  %> さんの勤務報告情報の新規登録</h2>

<!-- \workreports\new.html.erb (1) <<< -->
<% #debugger %>


<% if @workreport.errors.any? %>
  <u1 class="error_summary">
  <% @workreport.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
  </u1>
<% end %> 


<%= form_for [:admin, @workreport] do |form| %>
   <%= render "form", form: form %>
   <p><%= form.submit %></p>
<% end %>
